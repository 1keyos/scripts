# found in the LICENSE file.

echo "Create global_config.ini in autotest directory."

pushd ${STATEFUL_DIR}

cat > ${STATEFUL_DIR}/home/autotest-client/global_config.ini <<EOF
[CLIENT]
drop_caches: False
drop_caches_between_iterations: False
EOF

popd
